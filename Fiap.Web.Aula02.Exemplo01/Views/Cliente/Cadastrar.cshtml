@model Cliente 

@{ 
    ViewData["Title"] = "Cadastrar";
}

<h1>Cadastrar</h1>

@TempData["nome"]

@if(ViewBag.cliente != null)
{
<div>
    @ViewData["msg"]<br>
    @ViewData["nome"]
    @ViewBag.cpf
    @ViewBag.cliente.Nome;
    Nome: @Model.Nome Cpf: @Model.Cpf;
</div>
}

<form method="post">
    <div class="form-group">
        <label for="id-nome">Nome</label>
        <input type="text" name="nome" id="id-nome" class="form-control"/>
    </div>
    <div class="form-group">
        <label for="id-cpf">CPF</label>
        <input type="text" name="cpf" id="id-cpf" class="form-control"/>
    </div>
    <input type="submit" value="Cadastrar" class="btn btn-dark"/>
</form>

@section Scripts{ 
    <script>
        $().ready(function () {})
    </script>
}